<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="win.aladhims.PresenseMe">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="win.aladhims.PresenseMe.NewNgajarActivity"
            android:label="Bikin Ngajar"/>
        <activity android:name="win.aladhims.PresenseMe.DetailNgajarActivity"
            android:label="Detail Ngajar"/>
        <activity
            android:name="win.aladhims.PresenseMe.EditProfilMahasiswa"
            android:parentActivityName="win.aladhims.PresenseMe.MahasiswaDrawerActivity"
            android:label="Edit Profile"/>
        <activity
            android:name="win.aladhims.PresenseMe.SignInActivity"
            android:theme="@style/NoBarTheme"></activity>
        <activity
            android:name="win.aladhims.PresenseMe.EditProfilDosen"
            android:parentActivityName="win.aladhims.PresenseMe.DosenDrawerActivity"
            android:label="Edit Profile"/>
        <activity
            android:name="win.aladhims.PresenseMe.DosenDrawerActivity"
            android:theme="@style/NoBarTheme" />
        <activity
            android:name="win.aladhims.PresenseMe.MahasiswaDrawerActivity"
            android:theme="@style/NoBarTheme" />
        <activity android:name="win.aladhims.PresenseMe.DetailProfil"
            android:label="Detail Profile"/>
        <activity
            android:name="win.aladhims.PresenseMe.ChatActivity"
            android:parentActivityName="win.aladhims.PresenseMe.SignInActivity" />
        <activity
            android:name="win.aladhims.PresenseMe.SplashScreen"
            android:theme="@style/NoBarTheme">
            <intent-filter android:label="Pre SenseMe">
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="win.aladhims.PresenseMe.WelcomeActivity"
            android:theme="@style/NoBarTheme"></activity>
    </application>

</manifest>