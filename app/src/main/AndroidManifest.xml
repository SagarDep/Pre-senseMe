<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="win.aladhims.presensigeofence">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MatkulListActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".NewNgajarActivity" />
        <activity android:name=".DetailNgajarActivity" />
        <activity android:name=".EditProfilMahasiswa"
            android:parentActivityName=".MahasiswaDrawerActivity"/>
        <activity
            android:name=".SignInActivity"
            android:theme="@style/NoBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EditProfilDosen"
            android:parentActivityName=".DosenDrawerActivity" />
        <activity
            android:name=".DosenDrawerActivity"
            android:theme="@style/NoBarTheme" />
        <activity
            android:name=".MahasiswaDrawerActivity"
            android:theme="@style/NoBarTheme" />
        <activity android:name=".DetailProfil"></activity>
    </application>

</manifest>